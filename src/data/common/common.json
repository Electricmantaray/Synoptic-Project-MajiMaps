{
    "meta": {
        "charset": "utf-8",
        "viewport": "width=device-width, initial-scale=1.0",
        "title": "MajiMaps"
    },
    "styles": {
        "leafletCSS": "https://unpkg.com/leaflet@1.9.4/dist/leaflet.css",
        "tailwindCSS": "/css/output.css"
    },
    "scripts": {
        "leafletJS": "https://unpkg.com/leaflet@1.9.4/dist/leaflet.js"
    },
    "logo": {
        "src": "/images/Minimal - MajiMapsIcon.png",
        "alt": "MajiMaps",
        "style": "h-[60px] w-auto object-contain mx-auto sm:mx-0"
    },
    "adminHeader": {
        "id": "adminHeader",
        "container": {
            "style": "flex items-center justify-between bg-[#e5e5eb] shadow-md px-6 py-4 rounded-b-[15px]"
        },
        "left": {
            "text": {
                "content": "Admin Panel",
                "style": "text-gray-800 text-xl font-semibold ml-4 select-none"
            },
            "containerStyle": "flex items-center"
        },
        "right": {
            "logoutButton": {
                "text": "Logout",
                "style": "bg-red-600 hover:bg-red-700 text-white px-4 py-2 rounded-md font-medium transition-colors duration-200",
                "type": "button",
                "id": "logoutBtn"
            }
        }
    },
    "navigation": {
        "style": "transition-colors hover:text-blue-600",
        "links": [
            {
                "label": "Home",
                "href": "#home"
            },
            {
                "label": "Map",
                "href": "#maps"
            },
            {
                "label": "Report",
                "href": "#report"
            },
            {
                "label": "Contact",
                "href": "#contact"
            }
        ]
    },
    "footer": {
        "text": "Maji Maps. All rights reserved",
        "email": "majimaps@gmail.com"
    }
}